{
  "collectionName": "components_sections_parallaxes",
  "info": {
    "displayName": "Parallax Section",
    "icon": "layer",
    "description": "Parallax scrolling section with image and content"
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "backgroundImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "height": {
      "type": "enumeration",
      "enum": ["small", "medium", "large", "full"],
      "default": "medium"
    },
    "speed": {
      "type": "decimal",
      "default": 0.5,
      "min": 0,
      "max": 1
    },
    "overlay": {
      "type": "boolean",
      "default": true
    }
  }
}
